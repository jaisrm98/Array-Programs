package coding;

public class All_Subarray {

	public static void main(String[] args) {
		int A[] = {1,2,3 };

		int n = A.length;
		int sum = 0;
		int total = 0;
		for (int i = 0; i < n; i++) {
			sum = 0;
			for (int j = i; j < n; j++) {
				for (int k = i; k <= j; k++) {
					System.out.print(A[k] + " ");

				}

				System.out.println("\n");
			}
			

		}

	}

}


Output: 
1 

1 2 

1 2 3 

2 

2 3 

3 


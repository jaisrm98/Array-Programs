import java.util.*;
public class MyClass {
    public static void main(String args[]) {
     int arr1[]= {1,2,3,4};
	int arr2[]= {1,2,6,7,4};
    ArrayList<Integer>list=new ArrayList<Integer>();
    for(int i:arr1)
    list.add(i);
    for(int i:arr2)
    {
        if(list.contains(i))
        System.out.print(i+" ");
    }
	
    }
}


Output:  1 2 4

package Practice;

import java.util.*;

public class Max_elements_in_two_arrays {

	public static void main(String[] args) {
	int arr1[]= {1,2,3,4};
	int arr2[]= {1,2,6,7,4};
	ArrayList<Integer>list=new ArrayList<Integer>();
	for(int i:arr2)
		list.add(i);
		
	for(int i=0;i<arr1.length;i++)
	{
		if(list.contains(arr1[i]))
			System.out.print(arr1[i]+" ");
	}

	}

}


Output:  1 2 4

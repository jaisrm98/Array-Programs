package coding;

public class Prefix_Sum {
	public static void main(String[] args) {
		int arr[] = { -3,6,2,4,5,2,8,-9,3,1 };
		
		for(int i=0;i<arr.length;i++)
		{
			
			if(i==0)
				arr[i]=arr[0];
			else
			arr[i]=arr[i-1]+arr[i];
		}
		
		for(int i=0;i<arr.length;i++)
			System.out.print(arr[i]+" ");
		
		

	}
}

Output: 
-3 3 5 9 14 16 24 15 18 19 

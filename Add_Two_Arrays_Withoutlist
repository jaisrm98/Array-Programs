package Practice;
import java.util.*;
public class Concatenate_Array {

	public static void main(String[] args) {
		int arr1[]= {1,2};
		int arr2[]= {3,4};
		int arr3[]=new int[arr1.length+arr2.length];
		int k=0;
		for(int i=0;i<arr1.length;i++)
		{
			arr3[k++]=arr1[i];
		}
		for(int i=0;i<arr2.length;i++)
		{
			arr3[k++]=arr2[i];
		}
		
		
		for(int i:arr3)
			System.out.print(i+" ");
	}
	
}


Output: 1 2 3 4

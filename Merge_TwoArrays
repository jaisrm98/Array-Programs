package Practice;
import java.util.*;
public class Concatenate_Array {

	public static void main(String[] args) {
		int arr1[]= {1,2};
		int arr2[]= {3,4};
		ArrayList<Integer>list=new ArrayList<Integer>();
		for( int i:arr1)
			list.add(i);
		
		for( int i:arr2)
			list.add(i);
		
		
		for(int i:list)
			System.out.print(i+" ");
	}

}


Oytput: 1 2 3 4

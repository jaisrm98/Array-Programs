import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      int arr[]= {1,1,2,3};
	ArrayList<Integer>list=new ArrayList<Integer>();
	int sum=0;
		
		for(int i=0;i<arr.length;i++)
		{
			if(!list.contains(arr[i]))
			{
			
        
        list.add(arr[i]);
        sum+=arr[i];
        }
			else
			{
				sum=sum-arr[i];
			}
		}
	
	
		System.out.println(sum);
    }
}

Input: 1,1,2,3
Output: 5
